    void testGetNext()
    {
        Real start = 2.0;
        Real end = 5.0;
        Real step = 1.0;

        SysSequenceReal sequence = SysSequenceReal::construct(start, end, step);

        this.assertEquals(start + step, sequence.getNext(start));
        this.assertEquals(end + step, sequence.getNext(end));
    }

